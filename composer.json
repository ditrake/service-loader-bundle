{
  "name": "srr/service-loader-bundle",
  "description": "Service for load multi autowire services",
  "type": "symfony-bundle",
  "version": "0.1.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Razin",
      "email": "srr@webcrt.ru"
    }
  ],
  "require": {
    "php": "^7.2",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "^4.*",
    "symfony/yaml": "^4.*",
    "sensio/framework-extra-bundle": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "srr\\ServiceLoader\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "srr\\ServiceLoader\\Tests" : "tests/"
    }
  },
  "extra": {
    "symfony" : {
      "allow-contrib" : false,
      "require" : "4.0.*"
    }
  }
}